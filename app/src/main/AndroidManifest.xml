<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.pluu.sample.activityresult">

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ResultSecondActivity" />
        <activity android:name=".ActivityResultSampleActivity" />
        <activity android:name=".ActivityResultSampleFragmentActivity" />

        <activity android:name=".OldActivityResultSampleActivity" />
        <activity android:name=".OldActivityResultSampleFragmentActivity" />
    </application>

</manifest>